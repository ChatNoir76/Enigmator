<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <Pattern>[%level] %msg%n</Pattern>
        </encoder>
    </appender>

    <logger name="fr.chatnoir.enigmator" level="debug" />

    <root level="debug">
        <appender-ref ref="STDOUT" />
    </root>

</configuration>